{
  "expo": {
    "name": "Barber Booking App",
    "slug": "barber-booking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/barber-app-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/barber-app-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "446cf189-6c5c-4874-be61-0beec3d47453"
      }
    },
    "android": {
      "package": "com.hussainhamim.barberbookingapp"
    }
  }
}
